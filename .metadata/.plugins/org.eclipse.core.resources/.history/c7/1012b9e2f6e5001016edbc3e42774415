package com.ripplesystem.foobar.command;

public class FBGetStoreInfo extends FBCommand
{
	
	public class Response extends FBCommandResponse
	{
		public static final int FAILCODE_STORE_NOT_FOUND = 1;
		
		private StoreInfo storeInfo;
		
		public Response(boolean success)
		{
			super(success);
		}
		
		public StoreInfo getStoreInfo() { return storeInfo; }
		public void setStoreInfo(StoreInfo value) { storeInfo = value; }
	}
}
