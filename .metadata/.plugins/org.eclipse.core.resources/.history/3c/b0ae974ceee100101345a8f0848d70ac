package com.ripplesystem.foobar.model;

import com.ripplesystem.model.RSEntity;

/**
 * Object representation of Store.
 * @author izumi
 *
 */
@RSEntity
public class StoreInfo
{
	public static final String STORE = "StoreInfo";
	public static final String FLD_NAME = "name";
	public static final String FLD_ADDRESS = "address";
	public static final String FLD_TEL = "tel";
	public static final String FLD_URL = "url";
	public static final String FLD_IMAGE_URL = "imageUrl";
	public static final String FLD_EMAIL = "email";
	public static final String FLD_PASSWORD = "password";
	public static final String FLD_IS_EMAIL_VERIFIED = "isEmailVerified";
	
	
}
